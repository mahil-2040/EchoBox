# Flutter Frontend Environment Variables Example
# Copy this file to .env and update the values as needed

# Server Configuration
SERVER_IP=localhost
SERVER_PORT=9000
SERVER_URL=http://localhost:9000

# App Configuration
APP_NAME=P2P File Transfer
APP_VERSION=1.0.0
DEBUG_MODE=true

# Window Configuration (for desktop)
WINDOW_WIDTH=600
WINDOW_HEIGHT=600
WINDOW_MIN_WIDTH=600
WINDOW_MIN_HEIGHT=600

# File Transfer Configuration
DEFAULT_RECEIVE_FOLDER=Downloads
DEFAULT_SHARE_FOLDER=Shared

# Socket Configuration
SOCKET_TIMEOUT=5000
RECONNECTION_ATTEMPTS=3
RECONNECTION_DELAY=1000

# Development Configuration
ENABLE_LOGGING=true
LOG_LEVEL=debug